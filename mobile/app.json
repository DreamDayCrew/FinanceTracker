{
  "expo": {
    "name": "My Tracker",
    "slug": "finance-tracker",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#16a34a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "permissions": [
      "READ_SMS",
      "RECEIVE_SMS",
      "SEND_SMS"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.mytracker.finance"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#16a34a"
      },
      "package": "com.mytracker.finance",
      "permissions": ["READ_SMS", "RECEIVE_SMS", "SEND_SMS"]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "739426fe-dbee-403b-9475-38febc609978"
      }
    }
  }
}
